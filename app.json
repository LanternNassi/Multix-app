{
  "expo": {
    "name": "Multix",
    "slug": "Multix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Notifications.png",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/Notifications.png",
          "mode": "production",
          "color": "#000000"
        }
      ] ,
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "splash": {
      "image": "./assets/Notifications.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lantern.Multix"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Notifications.png",
        "backgroundColor": "#0000000"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.lantern.Multix"
    },
    "web": {
      "favicon": "./assets/Notifications.png"
    }
  }
}
